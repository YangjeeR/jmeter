          <stringProp name="WebDriverSampler.script">var pkg=JavaImporter(org.openqa.selenium) //import java selenium package
var support_ui=JavaImporter(org.openqa.selenium.support.ui.WebDriverWait); //import WebDriverWait Package
var ui=JavaImporter(org.openqa.selenium.support.ui); //import Selenium Support UI package
var wait=new support_ui.WebDriverWait(WDS.browser,5000); // wait 5 seconds until page loads
WDS.sampleResult.sampleStart() //sample starting point
WDS.browser.get(&apos;http://kinstaging.graphenecreative.co.uk/&apos;) // opens website \
WDS.log.info(&quot;Navigated to kin site&quot;);
var wait=new support_ui.WebDriverWait(WDS.browser,5000);

var login=WDS.browser.findElement(pkg.By.xpath(&quot;//div[@class=&apos;sign-up-btn&apos;]/a&quot;));
login.click();
WDS.log.info(&quot;sign up button has been clicked.&quot;);

var login=WDS.browser.findElement(pkg.By.xpath(&quot;//div[@class=&apos;sign-up-btn&apos;]/a&quot;));
login.click();

var wait=new support_ui.WebDriverWait(WDS.browser,5000);

var first = WDS.browser.findElement(pkg.By.id(&apos;first-name&apos;)); //define username field
first.sendKeys([&apos;student1&apos;]) ;// types word “student1” in field
var last = WDS.browser.findElement(pkg.By.id(&apos;last-name&apos;)) ;
last.sendKeys([&apos;student1&apos;]) ;
var usernameField = WDS.browser.findElement(pkg.By.id(&apos;username&apos;)); //define username field
usernameField.sendKeys([&apos;TestY&apos;]); // types word “student1” in field
var email = WDS.browser.findElement(pkg.By.id(&apos;email&apos;)); //define username field
email.sendKeys([&apos;email@gmail.com&apos;]); // types word “student1” in field
var passwordField = WDS.browser.findElement(pkg.By.id(&apos;password&apos;)) 
passwordField.sendKeys([&apos;123&apos;]); 
var confirm = WDS.browser.findElement(pkg.By.id(&apos;confirm-password&apos;));
confirm.sendKeys([&apos;123&apos;]) ;
var signUp=WDS.browser.findElement(pkg.By.xpath(&apos;/html/body/div[3]/main/div/div/div/div[2]/div/div[2]/form/div[7]/button]&apos;)); //define login submit button
signUp.click(); //click on the login button
WDS.log.info(WDS.usernameField + &apos; has registered &apos;) //log message
WDS.sampleResult.sampleEnd()</stringProp>
